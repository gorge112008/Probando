<h4>*Productos Disponibles:</h4>
<div class="container-fluid">
  <div class="row">
    <div class="col m14">
      <div class="col s4 m12" id="contenedor">
        {{#each response}}
          <div class="col s4 m4">
            <div class="card">
              <div class="card-image">
                <img class="responsive-img" src={{thumbnail}} />
                <span class="card-title">{{tittle}}</span>
              </div>
              <div class="card-content">
                <b>
                  {{description}}
                </b>
                <p>{{price}}</p>
                <b>Code: <b class="code">{{code}}</b></b>
              </div>
            </div>
          </div>
        {{/each}}
      </div>
    </div>
  </div>
</div>
<script
  src="https://cdn.jsdelivr.net/npm/sweetalert2@11.4.24/dist/sweetalert2.all.min.js"
></script>
<script
  src="https://cdnjs.cloudflare.com/ajax/libs/materialize/1.0.0/js/materialize.min.js"
></script>
   